{
  "name": "RJ Services",
  "short_name": "RJ Services",
  "description": "Your trusted accounting and bookkeeping consultancy offering various services including ITR, GST, and PAN card assistance.",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4CAF50",
  "orientation": "portrait",
  "icons": [
    {
      "src": "img/icon-96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "img/icon-128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "img/icon-256.png",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "img/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "categories": ["business", "productivity"],
  "lang": "en-US",
  "iarc_rating_id": "YOUR_IARC_RATING_ID", // Replace with a valid IARC Rating ID
  "launch_handler": {
    "client_mode": "multiple",
    "web": {
      "url": "https://rj236129.github.io/"
    }
  },
 "file_handlers": {
    "application/pdf": {
      "action": "/handle-file",
      "name": "PDF Document"
    },
    "application/vnd.openxmlformats-officedocument.wordprocessingml.document": {
      "action": "/handle-file",
      "name": "Word Document"
    },
    "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": {
      "action": "/handle-file",
      "name": "Excel Spreadsheet"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+services",
      "url": "https://rj236129.github.io/?url=%s"
    }
  ],
   "scope_extensions": [
    {
      "origin": "https://example.com",
      "url_pattern": "/path/*"
    },
    {
      "origin": "https://another-example.com",
      "url_pattern": "/resources/*"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "display_override": ["window-controls-overlay"],
  "edge_side_panel": true,
  "background_sync": {
    "min_interval": 60
  },
  "screenshots": [
    {
      "src": "img/screenshot-1.png",
      "sizes": "1280x720",
      "type": "image/png"
    },
    {
      "src": "img/screenshot-2.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ]
}
