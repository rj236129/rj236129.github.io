{
  "id": "https://rj236129.github.io/",
  "name": "RJ Services",
  "short_name": "RJ Services",
  "description": "Expert accounting and bookkeeping consultancy services.",
  "start_url": "/index.html",
  "scope": "/",
  "display_override": ["window-controls-overlay", "standalone"],
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "orientation": "portrait",
  "window_controls_overlay": {
    "enabled": true
  },
  "edge_side_panel": {
    "enabled": true,
    "preferred_width": 350
  },
  "file_handlers": [
    {
      "action": "/file-handler",
      "accept": {
        "application/pdf": [".pdf"],
        "image/png": [".png"]
      }
    }
  ],
  "handle_links": {
    "url_patterns": ["*://yourwebsite.com/*"]
  },
  "protocol_handlers": [
    {
      "protocol": "web+example",
      "url": "/handle/%s"
    }
  ],
  "widgets": [
    {
      "name": "Weather Widget",
      "url": "/widgets/weather"
    },
    {
      "name": "Tasks Widget",
      "url": "/widgets/tasks"
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-new",
    "launch_type": "existing"
  },
  "background_sync": {
    "sync_tag": "sync-updates"
  },
  "categories": ["business", "productivity", "utilities"],
  "periodic_sync": {
    "min_interval": 86400
  },
  "push_notifications": {
    "enabled": true
  },
  "icons": [
    {
      "src": "icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "iarc_rating_id": "your-iarc-rating-id",  // If applicable
  "lang": "en",
  "prefer_related_applications": false
}
