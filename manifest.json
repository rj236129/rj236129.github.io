{
  "id": "https://rj236129.github.io/",
  "name": "RJ Services",
  "short_name": "RJ Services",
  "description": "Expert accounting and bookkeeping consultancy services, including PAN card assistance and government job applications.",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "orientation": "portrait",
  "launch_handler": {
    "client_mode": "window",
    "url": "/index.html"
  },
  "display_override": ["window-controls-overlay"],
  "edge_side_panel": {
    "enabled": true
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "types": [
        {
          "kind": "file",
          "mime": "application/pdf"
        },
        {
          "kind": "file",
          "mime": "image/jpeg"
        }
      ]
    }
  ],
  "handle_links": {
    "url_pattern": ["*://yourwebsite.com/*"]
  },
  "protocol_handlers": [
    {
      "protocol": "web+yourprotocol",
      "url": "/open-protocol?url=%s"
    }
  ],
  "icons": [
    {
      "src": "icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
