{
  "name": "RJ Services",
  "short_name": "RJ Services",
  "description": "Your one-stop solution for accounting and bookkeeping services.",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4A90E2",
  "orientation": "portrait",
  "scope": "/",
  "icons": [
    {
      "src": "img/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "img/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "serviceworker": {
    "src": "service-worker.js",
    "scope": "/"
  },
  "file_handlers": {
    "application/pdf": {
      "action": "/view-pdf.html",
      "icons": [
        {
          "src": "img/pdf-icon.png",
          "sizes": "192x192"
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+myapp",
      "url": "/?url=%s"
    }
  ],
  "categories": ["business", "finance", "consulting"],
  "iarc_rating_id": "E",
  "lang": "en-US",
  "dir": "ltr",
  "widgets": [
    {
      "src": "img/widget-icon.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ]
}
